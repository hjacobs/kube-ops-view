  "dependencies": {
    "pixi.js": "^4.3.2",
    "babel-runtime": "^6.20.0",
    "babel-polyfill": "^6.20.0"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "brfs": "^1.4.3",
    "eslint": "^3.13.1",
    "eslint-loader": "^1.6.1",
    "rimraf": "^2.5.4",
    "transform-loader": "^0.2.3",
    "webpack": "^1.14.0"

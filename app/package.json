{
  "name": "kube-ops-view",
  "version": "1.0.0",
  "description": "=========================== Kubernetes Operational View ===========================",
  "main": "src/app.js",
  "config": {
    "buildDir": "../kube_ops_view/static/build"
  },
  "scripts": {
    "prestart": "npm install",
    "start": "NODE_ENV=development webpack --watch",
    "webpack": "webpack -p --config ./webpack.config.js",
